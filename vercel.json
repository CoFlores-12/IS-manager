{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
    }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.js"
    },
    {
        "src": "/",
        "dest": "public/index.html"
    },
    {
        "src": "/(.+)",
        "dest": "public/$1"
    }
    ]
  }