{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        },
        {
          "src": "public/**",
          "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "index.py"
      },
      {
          "src": "/",
          "dest": "public/index.html"
      },
      {
          "src": "/(.+)",
          "dest": "public/$1"
      }
      ]
}